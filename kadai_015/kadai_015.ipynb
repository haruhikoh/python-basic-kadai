        "class Human:\n",
        "  def __init__(self, name, age):\n",
        "    self.name = name\n",
        "    self.age = age\n",
        "\n",
        "  def printinfo(self):\n",
        "     print(f\"{self.name},{self.age}\")\n",
        "\n",
        "human = Human(\"name\", \"age\")\n",
        "\n",
        "human.printinfo()\n"
